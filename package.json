{
  "name": "mojocss",
  "version": "0.2.0-rc.12",
  "description": "The Atomic CSS Framework for crafting gorgeous UIs without shipping any CSS.",
  "main": "index.js",
  "unpkg": "dist/mojo.min.js",
  "keywords": [
    "atomic-css",
    "responsive",
    "grid-system",
    "flexbox",
    "css-grid",
    "lightweight",
    "modular",
    "customizable",
    "theming",
    "accessibility",
    "cross-browser",
    "css-variables",
    "mobile-first",
    "minimalist",
    "scalable",
    "utility-first"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mojocss/mojocss.git"
  },
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "lint": "eslint ."
  },
  "author": "Mojo CSS Authors mojocss.com",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "transform": {
      "\\.js$": "@swc/jest"
    }
  },
  "devDependencies": {
    "@swc/core": "^1.2.133",
    "@swc/jest": "^0.2.36",
    "esbuild": "^0.14.13",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "terser-webpack-plugin": "^5.3.10"
  },
  "type": "module",
  "dependencies": {
    "jsdom": "^24.0.0"
  }
}
